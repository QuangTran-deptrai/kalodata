$gitignoreContent = @"
# Python
__pycache__/
*.py[cod]
*$py.class

# Virtual Env
venv/
env/
.venv/

# Secrets
.env

# Data Output
*.xlsx
*.csv
kalodata_master.xlsx

# IDE & OS
.vscode/
.idea/
.DS_Store
Thumbs.db
*.log

# Docker Logs - GIỮ LẠI .gitkeep
logs/*
!logs/.gitkeep
"@

Set-Content .gitignore $gitignoreContent -Encoding UTF8